{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
      "linebreak-style": 0,
      "no-console": 0,
      "no-underscore-dangle": 0,
      "consistent-return": 0,
      "no-restricted-globals": 0,
      "no-unused-vars": 0
    }
}
